{
  "name": "manktest",
  "description": "Common react web components and wrappers",
  "version": "1.0.6",
  "main": "lib/index.esm.js",
  "module": "lib/index.esm.js",
  "source": "src/index.js",
  "sideEffects": false,
  "scripts": {
    "clean:build": "rimraf lib",
    "clean:coverage": "rimraf coverage",
    "clean:storybook": "rimraf storybook-static",
    "clean": "npm run clean:build && npm run clean:coverage && npm run clean:storybook",
    "cleanup": "rimraf node_modules && npm run clean",
    "test": "jest",
    "test:update": "jest -u",
    "test:staged": "jest --bail --findRelatedTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --collectCoverageFrom='./src/**'",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "storybook:build": "npm run clean:storybook && NODE_OPTIONS=--openssl-legacy-provider build-storybook",
    "storybook:serve": "http-server storybook-static",
    "dev": "rollup -c rollup.config.js -w",
    "build": "npm run clean && rollup -c rollup.config.js",
    "lint": "eslint '**/*.js'",
    "lint:errors": "eslint --quiet '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix",
    "prettier:write": "prettier --write .",
    "prettier:check": "prettier --check .",
    "precommit": "lint-staged --config 'lint-staged.config.js'"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "classnames": "^2.3.1",
    "immer": "^9.0.12",
    "luxon": "^2.3.1",
    "react-datepicker": "^4.7.0",
    "react-table": "^7.7.0",
    "react-tooltip": "^4.2.21",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.8",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-docs": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "6.4.19",
    "@storybook/react": "^6.4.19",
    "@svgr/rollup": "^6.1.2",
    "@teachmint/eslint-config": "file:../eslint-config",
    "@teachmint/events": "file:../events",
    "@teachmint/prettier-config": "file:../prettier-config",
    "@testing-library/jest-dom": "^5.16.1",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.1.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.8",
    "postcss-preset-env": "^7.4.2",
    "prettier": "2.5.1",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.63.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook-css-modules-preset": "^1.1.1"
  },
  "keywords": [],
  "author": "Man K",
  "license": "UNLICENSED",
  "homepage": "https://github.com/manjunathgk22/testpackage",
  "files": [
    "lib"
  ]
}
